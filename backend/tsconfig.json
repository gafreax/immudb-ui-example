{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["es2023"],
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "ts-node": {
    "esm": true
  },
  "tsc-alias": {
    "resolveFullPaths": true,
    "verbose": true
  }
}
